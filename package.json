{
  "name": "mobx-react-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for MobX + React project with JSX, ES6 and decorator compilation",
  "scripts": {
    "start": "parcel index.html",
    "lint:js": "eslint --ext js src --ignore-pattern src/stubs/*",
    "lint:jsx": "eslint --ext jsx src",
    "lint:scss": "sass-lint -c .scss-lint.yml -v",
    "lint": "yarn lint:jsx && yarn lint:js && yarn lint:scss",
    "test": "NODE_ENV=test DOMAIN=com jest -c ./jest.config.js --colors",
    "test:coverage": "DOMAIN=com jest -c ./jest.config.js --coverage --colors"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mobxjs/mobx-react-boilerplate.git"
  },
  "license": "MIT",
  "dependencies": {
    "@scorum/theme-bootstrap": "0.0.2-testdeps",
    "add": "^2.0.6",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "helpers": "^0.0.6",
    "loadable-components": "^2.2.3",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.5",
    "mobx-react-router": "^4.0.4",
    "ramda": "^0.25.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router-dom": "^4.3.1",
    "rxjs": "^6.2.2",
    "yarn": "^1.9.4"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-ramda": "1.6.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.3.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-ramda": "^2.5.1",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.5.0",
    "mobx-react-devtools": "^6.0.1",
    "node-sass": "^4.9.3",
    "parcel-bundler": "^1.9.7",
    "prop-types": "^15.6.2",
    "sass-lint": "^1.12.1"
  }
}
